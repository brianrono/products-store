{
	"$schema": "http://json.schemastore.org/package",
	"name": "products-store",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "NODE_ENV=development nodemon backend/index.js",
		"build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
		"start": "NODE_ENV=production node backend/index.js"
	},
	"engines":{
		"node": ">= 18.18.0 <19.0.0"
	},
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"dotenv": "^16.4.5",
		"express": "^4.21.0",
		"mongodb": "^6.9.0",
		"mongoose": "^8.7.0"
	},
	"devDependencies": {
		"nodemon": "^3.1.7"
	},
	"Transform": "AWS::Serverless-2016-10-31",
	"Resources": {
		"Layer": {
			"Type": "AWS::Serverless::LayerVersion",
			"Properties": {
				"Description": {
					"Fn::Sub": [
						"Stack ${AWS::StackName} Layer ${ResourceName}",
						{
							"ResourceName": "Layer"
						}
					]
				},
				"ContentUri": "src/Layer",
				"RetentionPolicy": "Retain"
			}
		},
		"Api2": {
			"Type": "AWS::Serverless::Api",
			"Properties": {
				"Name": {
					"Fn::Sub": [
						"${ResourceName} From Stack ${AWS::StackName}",
						{
							"ResourceName": "Api2"
						}
					]
				},
				"StageName": "Prod",
				"DefinitionBody": {
					"openapi": "3.0",
					"info": {},
					"paths": {
						"/": {
							"put": {
								"responses": {}
							}
						}
					}
				},
				"EndpointConfiguration": "REGIONAL",
				"TracingEnabled": true,
				"Cors": {
					"MaxAge": 5
				}
			}
		},
		"Stream": {
			"Type": "AWS::Kinesis::Stream",
			"Properties": {
				"StreamEncryption": {
					"EncryptionType": "KMS",
					"KeyId": "alias/aws/kinesis"
				},
				"StreamModeDetails": {
					"StreamMode": "ON_DEMAND"
				}
			}
		},
		"Analyzer": {
			"Type": "AWS::AccessAnalyzer::Analyzer",
			"Properties": {
				"Type": "<String>"
			}
		}
	}
}